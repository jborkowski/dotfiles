{
  "telemetry": {
    "diagnostics": false
  },
  "assistant": {
    "default_open_ai_model": "gpt-3.5-turbo",
    "provider": {
      "name": "openai",
      "default_model": "gpt-3.5-turbo"
    }
  },
  "theme": "One Dark",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "vim_mode": true
}
