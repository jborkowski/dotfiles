set $gnome-schema org.gnome.desktop.interface
exec_always {
    gsettings set $gnome-schema color-scheme 'prefer-dark'
    gsettings set $gnome-schema gtk-theme 'Duskfox-Dark'
    gsettings set $gnome-schema icon-theme 'Duskfox-Dark'
    gsettings set $gnome-schema cursor-theme 'Duskfox-Dark'
    gsettings set $gnome-schema font-name 'Inter 11'
}

# Duskfox Theme
client.focused          #a0a8cd #1e202f #a0a8cd #a0a8cd #1e202f
client.focused_inactive #2c2e3e #1e202f #cdcecf #cdcecf #1e202f
client.unfocused        #2c2e3e #1e202f #cdcecf #cdcecf #1e202f
client.urgent           #ff966c #1e202f #ff966c #ff966c #1e202f
client.placeholder      #1e202f #1e202f #cdcecf #cdcecf #1e202f
client.background       #1e202f

